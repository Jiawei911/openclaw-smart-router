{
    "models": {
        "providers": {
            "aliyun-dashscope": {
                "baseUrl": "https://dashscope.aliyuncs.com/compatible-mode/v1",
                "apiKey": "sk-YOUR-API-KEY-HERE",
                "api": "openai-completions",
                "models": [
                    {
                        "id": "qwen-max",
                        "name": "Qwen-Max (Reasoning)",
                        "contextWindow": 30000,
                        "maxTokens": 8192
                    },
                    {
                        "id": "qwen-flash",
                        "name": "Qwen-Flash (Simple)",
                        "contextWindow": 30000,
                        "maxTokens": 8192
                    }
                ]
            },
            "nvidia-deepseek": {
                "baseUrl": "https://integrate.api.nvidia.com/v1",
                "apiKey": "nvapi-YOUR-API-KEY-HERE",
                "api": "openai-completions",
                "models": [
                    {
                        "id": "deepseek-ai/deepseek-v3.2",
                        "name": "DeepSeek-V3",
                        "contextWindow": 64000,
                        "maxTokens": 8192
                    }
                ]
            },
            "minimax-portal": {
                "baseUrl": "https://api.minimaxi.com/anthropic",
                "apiKey": "YOUR-API-KEY-HERE",
                "api": "anthropic-messages",
                "models": [
                    {
                        "id": "MiniMax-M2.1",
                        "name": "MiniMax M2.1",
                        "contextWindow": 200000,
                        "maxTokens": 8192
                    }
                ]
            }
        }
    },
    "agents": {
        "defaults": {
            "model": {
                "primary": "aliyun-dashscope/qwen-max",
                "fallbacks": [
                    "aliyun-dashscope/qwen-flash"
                ]
            },
            "workspace": "~/.openclaw/workspace"
        }
    }
}